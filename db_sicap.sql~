  `codigo_mes` int(11) NOT NULL,
  `codigo_semana` int(11) NOT NULL,
  `codigo_articulo` int(11) NOT NULL,
  `cantidad` varchar(50) NOT NULL,
  `costo` varchar(50) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1 ;

